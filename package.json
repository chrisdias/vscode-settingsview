{
  "name": "vscode-50-settings",
  "displayName": "50 Settings",
  "description": "Quick access to 50 commonly used VS Code settings",
  "version": "1.0.0",
  "publisher": "chrisdias",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "settings",
    "configuration",
    "preferences",
    "customize",
    "productivity"
  ],
  "icon": "resources/icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisdias/vscode-50-settings"
  },
  "bugs": {
    "url": "https://github.com/chrisdias/vscode-50-settings/issues"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [{
      "command": "popularSettings.show",
      "title": "Show 50 Settings"
    }],
    "viewsContainers": {
      "activitybar": [{
        "id": "popularSettings",
        "title": "50 Settings",
        "icon": "$(gear)"
      }]
    },
    "views": {
      "popularSettings": [{
        "type": "webview",
        "id": "popularSettings.settingsView",
        "name": "50 Settings"
      }]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^16.0.0",
    "typescript": "^5.1.6"
  }
}

{
  "name": "vscode-popular-settings",
  "displayName": "Popular Settings View",
  "description": "Quick access to commonly used VS Code settings",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [{
      "command": "popularSettings.show",
      "title": "Show Popular Settings"
    }],
    "views": {
      "explorer": [{
        "type": "webview",
        "id": "popularSettings.settingsView",
        "name": "Popular Settings"
      }]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^16.0.0",
    "typescript": "^5.1.6"
  }
}
